{
  "name": "@mand-mobile/vue-cli-plugin-builder",
  "version": "3.0.1-alpha.9",
  "description": "mand-mobile 构建层封装",
  "keywords": ["mand-mobile", "buildtime"],
  "author": "lucker <byterotate@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": ["bin", "lib", "dist", "generator"],
  "scripts": {
    "prebuild": "rm -fr dist",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --collectCoverage=false",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testMatch": ["**/(*.)+(test|spec).ts"],
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  },
  "dependencies": {
    "@mand-mobile/components": "^3.0.1-alpha.9",
    "@mand-mobile/platform-builder": "^0.1.0",
    "@mand-mobile/shared": "^3.0.1-alpha.9",
    "@rollup/plugin-babel": "^5.0.4",
    "@vue/cli": "^4.4.4",
    "@vue/cli-plugin-babel": "^4.4.4",
    "@vue/cli-plugin-typescript": "^4.4.4",
    "@vue/cli-service": "^4.4.4",
    "@vue/cli-shared-utils": "^4.4.4",
    "@vue/cli-test-utils": "^4.4.4",
    "jest": "^26.0.1",
    "ramda": "^0.27.0",
    "ts-jest": "^26.1.0",
    "vue": "^2.6.11",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/ramda": "^0.27.6",
    "execa": "^4.0.3"
  }
}
